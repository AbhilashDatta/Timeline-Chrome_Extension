{
    "manifest_version":2,
    "name": "Timeline",
    "version":"1.0",
    "description": "See Timeline",
    "icons":{
        "128":"logo.png"
    },
    "browser_action":{
        "default_icon":"logo.png",
        "default_popup":"popup.html"
    },
    "options_page":"options.html",
    "background":{
        "scripts":["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "http://127.0.0.1:5000/",
        "tabs"
    ]
}